{
    "manifest_version": 3,
    "name": "Password Manager",
    "version": "0.1.0",
    "description": "Securely store and manage passwords for websites.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  